{
  "name": "Modified Rocker Dev Container",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "features": {
    "ghcr.io/MiguelRodo/DevContainerFeatures/repos:1": {},
    "ghcr.io/MiguelRodo/DevContainerFeatures/config-r:2": {
      "debug": true,
      "debugRenv": true
    },
    "ghcr.io/rocker-org/devcontainer-features/r-rig:1": {
      "installDevTools": false,
      "installREnv": true,
      "installRadian": true,
      "version": "none",
      "pakVersion": "auto",
      "vscodeRSupport": "minimal",
      "pandocVersion": "auto"
    },
    "ghcr.io/rocker-org/devcontainer-features/apt-packages:1": {
      "packages": "xvfb,vim,libcurl4-openssl-dev,libsecret-1-dev,jq"
    },
    "ghcr.io/rocker-org/devcontainer-features/quarto-cli:1": {
      "installTinyTex": true
    },
    "ghcr.io/devcontainers/features/aws-cli:1": {
      "version": "latest"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "quarto.quarto",
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "REditorSupport.r",
        "bierner.markdown-preview-github-styles",
        "mathematic.vscode-pdf"
      ]
    },
    "codespaces": {
      "repositories": {
        "SATVILab/stimgate": {
          "permissions": {
            "actions": "write",
            "contents": "write",
            "packages": "read",
            "workflows": "write"
          }
        },
        "SATVILab/Analysis24StimGate": {
          "permissions": {
            "actions": "write",
            "contents": "write",
            "packages": "read",
            "workflows": "write"
          }
        },
        "SATVILab/DataProcessed25PostMortemCyTOFCytokines": {
          "permissions": {
            "actions": "write",
            "contents": "write",
            "packages": "read",
            "workflows": "write"
          }
        },
        "SATVILab/DataTidyACSCyTOFCytokinesTCells": {
          "permissions": {
            "actions": "write",
            "contents": "write",
            "packages": "read",
            "workflows": "write"
          }
        },
        "SATVILab/Comp24StimGate": {
          "permissions": {
            "actions": "write",
            "contents": "write",
            "packages": "read",
            "workflows": "write"
          }
        },
        "SATVILab/UtilsCytoRSV": {
          "permissions": {
            "actions": "write",
            "contents": "write",
            "packages": "read",
            "workflows": "write"
          }
        }
      }
    }
  }
}
